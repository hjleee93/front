{
  "name": "zempie-front",
  "version": "0.0.1",
  "description": "게임 공유 플랫폼",
  "productName": "ZEMPIE",
  "author": "hg.park <hg.park@fromthered.com>",
  "private": true,
  "scripts": {
    "serve": "npm run download:i18n && cross-env QENV=local quasar dev ",
    "build-dev": "cross-env QENV=development quasar build",
    "build": "cross-env QENV=production quasar build",
    "build:pwa": "quasar build -m pwa",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
    "scan:i18n": "i18next-scanner --config i18next-scanner.config.js",
    "upload:i18n": "npm run scan:i18n && node translation_tool/upload.js",
    "download:i18n": "node translation_tool/download.js"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "firebase": "^8.0.0",
    "fullpage.js": "^3.1.1",
    "google-spreadsheet": "^3.1.15",
    "quasar": "^1.0.0",
    "vue-class-component": "^7.2.2",
    "vue-fullpage.js": "^0.1.7",
    "vue-gtag": "^1.10.0",
    "vue-gtm": "^3.4.0-vue2",
    "vue-i18n": "^8.0.0",
    "vue-meta": "^2.4.0",
    "vue-property-decorator": "^8.3.0",
    "vuedraggable": "^2.24.3",
    "webpack": "^4.44.2"
  },
  "devDependencies": {
    "@quasar/app": "^2.0.0",
    "@quasar/quasar-app-extension-qenv": "^1.0.3",
    "@types/node": "^10.17.15",
    "cross-env": "^7.0.2",
    "i18next-scanner": "^3.0.0"
  },
  "browserslist": [
    "ie >= 11",
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
